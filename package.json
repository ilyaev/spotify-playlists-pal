{
  "name": "pbartz.electron.spotify.companion",
  "version": "1.0.0",
  "description": "Spotify Companion",
  "main": "app/main.js",
  "scripts": {
    "clean": "touch ./app/tracks/a.b && rm -rf ./app/tracks/* && ",
    "build": "touch ./app/chump.map && rm ./app/*.map && webpack --config ./webpack.config.js --env prod",
    "dev": "webpack --config ./webpack.config.js --watch --env dev",
    "start": "electron ./app/main.js",
    "server": "cd server && yarn start",
    "run": "electron ./app/main.js dev",
    "pack": "electron-builder --dir",
    "dist": "yarn build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "shval": "yarn glslangValidator $1",
    "app": "./dist/mac/Spotify\\ Companion.app/Contents/MacOS/Spotify\\ Companion",
    "deliver": "open ./dist/Spotify\\ Companion-1.0.0.dmg"
  },
  "build": {
    "appId": "pbartz.electron.spotify.companion",
    "productName": "Spotify Companion",
    "files": [
      "**/*",
      "app/static/*",
      "static/*"
    ],
    "mac": {
      "category": "public.app-category.music",
      "target": "dmg",
      "icon": "app/static/play-icon.png"
    }
  },
  "author": "Yura Ilyaev<ilyaev@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-inlinesvg": "^1.0.0",
    "@types/spotify-web-api-node": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "devtron": "^1.4.0",
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "glslang-validator-prebuilt": "^0.0.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.11.0",
    "react-inlinesvg": "^1.1.7",
    "react-konva": "^16.10.1-0",
    "react-svg-loader": "^3.0.3",
    "spotify-web-api-node": "^4.0.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/d3-array": "^2.0.0",
    "@types/d3-scale": "^2.1.1",
    "@types/node-fetch": "^2.5.2",
    "@types/react-tooltip": "^3.9.3",
    "@types/three": "^0.103.2",
    "d3-array": "^2.3.3",
    "d3-scale": "^3.2.0",
    "electron-log": "^3.0.8",
    "electron-reload": "^1.5.0",
    "elemon": "^5.0.3",
    "express": "^4.17.1",
    "konva": "^4.0.16",
    "node-fetch": "^2.6.0",
    "performance-now": "^2.1.0",
    "react-contextmenu": "^2.12.0",
    "react-tooltip": "^3.11.1",
    "spotify-node-applescript": "^1.1.1",
    "three": "^0.109.0",
    "three.meshline": "^1.2.0",
    "webpack-glsl-loader": "^1.0.1"
  }
}